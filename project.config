[receive]
	requireChangeId = true
[project]
	state = active
[submit]
	mergeContent = true
[access "refs/heads/*"]
	label-Approved = +0..+1 group contrail-build-core
	label-Code-Review = -2..+2 group contrail-build-core
	workInProgress = group contrail-build-core
[access "refs/tags/*"]
	create = group contrail-build-core
	pushTag = group contrail-build-core
